{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "ZipKit",
  "colors": {
    "primary": "#000000",
    "light": "#FFFFFF",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Quickstart",
            "pages": ["index", "quickstart", "development"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://zipkit.io",
          "icon": "globe"
        }
      ]
    }
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get started",
      "href": "https://zipkit.io"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
